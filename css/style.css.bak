/* Default CSS start */
*{
	margin: 0;
	padding: 0;
	outline: 0;
	box-sizing: border-box;
}
.clear_fix{
	clear: both;
}
/* Default CSS end */
.heading_one{
	color: red;
	text-align: center;
	font-family: sans-serif;
}

/* Header Part */

#header{
	background-color: red;
}
.logo{
	width: 25%;
	float: left;
	background-color: lightgreen;
}
.menu{
	width: 75%;
	float: left;
	background-color: #17a00c;
}
.header_text{
	font-family: sans-serif;
	font-size: 30px;
	padding: 20px;
	text-align: center;
}

/* Banner Part */

#banner{
	width: 100%;
}
img{
	width: 100%;
	height: 400px;
}

/* Main Content Part */

#main_content{
	background-color: yellow;
}
.main{
	width: 75%;
}
.side{
	width: 25%;
}

#footer {
	background-color: purple;
}